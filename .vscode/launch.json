	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${file}",	// 表示当前文件
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": ["--nolazy"],
			"env": {
				"NODE_ENV": "development"
			},
			"preLaunchTask": "",
			"sourceMaps": false,
		}, {
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}